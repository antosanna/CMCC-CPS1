export CONDA_PREFIX=/users_home/csp/$USER/miniconda/
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/users_home/csp/$USER/miniconda/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/users_home/csp/$USER/miniconda/etc/profile.d/conda.sh" ]; then
        . "/users_home/csp/$USER/miniconda/etc/profile.d/conda.sh"
    else
        export PATH="/users_home/csp/$USER/miniconda/bin:$PATH"
    fi
fi
unset __conda_setup

if [ -f "/users_home/csp/$USER/miniconda/etc/profile.d/mamba.sh" ]; then
    . "/users_home/csp/$USER/miniconda/etc/profile.d/mamba.sh"
fi
# <<< conda initialize <<<
